{
	"name": "db/balatinac-site",
	"description": "Portfolio/Blog website made in TYPO3.",

	"repositories": [
		{
			"type": "path",
			"url": "./packages/*"
		}
	],

	"require": {
		"typo3/minimal": "^11.5",
		"typo3/cms-fluid-styled-content": "^11.5",
		"typo3/cms-t3editor": "^11.5",
		"typo3/cms-tstemplate": "^11.5",
		"typo3/cms-rte-ckeditor": "^11.5",
		"helhum/typo3-secure-web": "v0.3.2",
		"helhum/typo3-console": "v7.0.3",
		"db/balatinac-sitepackage": "1.0.0"
	},

	"suggest": {
		"typo3/cms-dashboard": "*"
	},

	"config": {
		"platform": {
			"php": "7.4.24"
		}
	},

	"extra": {
		"typo3/cms": {
			"root-dir": "private",
			"web-dir": "public"
		}
	}
}
